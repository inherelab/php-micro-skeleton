{
  "name": "inhere/micro-skeleton",
  "type": "project",
  "description": "a php micro application skeleton of the php",
  "keywords": [
    "cms",
    "blog"
  ],
  "homepage": "https://github.com/inhere/php-micro-skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "inhere",
      "email": "in.798@qq.com",
      "homepage": "http://www.yzone.net/"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/promises": "^1.3",
    "inhere/asset-manager": "dev-master",
    "inhere/library": "dev-master",
    "inhere/console": "dev-master",
    "inhere/database": "dev-master",
    "inhere/sroute": "dev-master",
    "inhere/lite-cache": "dev-master",
    "inhere/lite-db": "dev-master",
    "inhere/micro-framework": "dev-master",
    "inhere/simple-active-record": "dev-master",
    "inhere/process": "dev-master",
    "inhere/server": "dev-master",
    "inhere/code-generator": "dev-master",
    "swoole-kit/http-server": "dev-master",
    "swoole-kit/websocket-server": "dev-master",
    "toolkit/toolkit": "dev-master as 1.0",
    "toolkit/toolkit-plus": "dev-master",
    "monolog/monolog": "^1.2",
    "overtrue/pinyin": "^3.0",
    "phpfastcache/phpfastcache": "^6.0",
    "symfony/process": "^3.3"
  },
  "require-dev": {
    "inhere/simple-print-tool": "dev-master",
    "ulue/annotations": "dev-master",
    "zircote/swagger-php": "^2.0"
  },
  "autoload": {
    "files": [
      "./app/Helper/Functions.php"
    ],
    "psr-4": {
      "App\\": "./app/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "0": {
      "type": "git",
      "url": "https://github.com/inhere/php-database"
    },
    "1": {
      "type": "git",
      "url": "https://github.com/inhere/php-process"
    },
    "2": {
      "type": "git",
      "url": "https://github.com/inhere/code-generator"
    }
  },
  "config": {
    "secure-http": false
  }
}
